<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PasswordView">

        <!-- 是否显示忘记密码 -->
        <attr name="forget" format="boolean" />
        <!-- 忘记密码文本 -->
        <attr name="forget_text" format="string" />
        <!-- 密码提示文本 -->
        <attr name="tip_text" format="string" />
        <!-- 数字随机排列 -->
        <attr name="random_number" format="boolean" />

    </declare-styleable>

</resources>